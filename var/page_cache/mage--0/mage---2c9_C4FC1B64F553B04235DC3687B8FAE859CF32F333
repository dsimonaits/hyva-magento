          }).map(item => {\n                    item.setAttribute('aria-current', 'page');\n                    item.closest('li.level-0').setAttribute('data-active', 'true');\n                });\n            },\n            openMenuOnClick(menuNode) {\n                if (menuNode === this.hoverPanelActiveId) {\n                    this.hoverPanelActiveId = 0;\n                } else {\n                    this.hoverPanelActiveId = menuNode\n                }\n            }\n        }\n    }\n<\/script>\n\n        <div class=\"flex items-center gap-2 md:gap-1 order-3 md:-mr-1\">\n            <!--Compare Icon-->\n            <a id=\"compare-link\"\n               class=\"relative inline-block rounded p-1 hover:bg-primary\/10 outline-offset-2 invisible\"\n               :class=\"{ 'invisible': !(itemCount > 0) }\"\n               href=\"http:\/\/hyva-magento.local\/catalog\/product_compare\/index\/\"\n               title=\"Compare&#x20;Products\"\n               x-data=\"initCompareHeader()\"\n               @private-content-loaded.window=\"receiveCompareData($event.detail.data)\"\n               :aria-label=\"`\n                    Compare&#x20;Products,\n                    ${itemCount > 1\n                        ? hyva.str('\\u00251\\u0020Items', itemCount)\n                        : hyva.str('\\u00251\\u0020Item', itemCount)\n                    }`\"\n            >\n                <svg xmlns=\"http:\/\/www.w3.org\/2000\/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke-width=\"2\" stroke=\"currentColor\" aria-hidden=\"true\" class=\"md:h-6 md:w-6\" width=\"28\" height=\"28\">\n  <path stroke-linecap=\"round\" stroke-linejoin=\"round\" d=\"M3 6l3 1m0 0l-3 9a5.002 5.002 0 006.001 0M6 7l3 9M6 7l6-2m6 2l3-1m-3 1l-3 9a5.002 5.002 0 006.001 0M18 7l3 9m-3-9l-6-2m0-2v2m0 16V5m0 16H9m3 0h3\"\/>\n<\/svg>\n                <span\n                    x-text=\"itemCount\"\n                    class=\"absolute -top-1.5 -right-1.5 h-5 px-2 py-1 rounded-full bg-yellow-500 text